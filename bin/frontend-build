#!/bin/bash


cd /var/www
npm version     #Echo's NPM version



sudo npm install --global yo
yo --version 
sudo npm install --global bower
bower --version 
sudo npm install --global grunt-cli
grunt --version
sudo npm install grunt --save-dev
sudo npm install --global generator-angular


echo "yo" 
yo --version
echo "bower" 
bower --version
echo "grunt"
grunt --version